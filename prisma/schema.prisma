// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Account {
  id String @id @db.Uuid()
  cpf String @unique
  name String
  birthdate String
  phone String
  email String @unique
  password String
 
  @@map("accounts")
}